{
	"name" : "taobig/yii-helper",
	"description" : "yii components",
	"type" : "library",
	"license" : "MIT",
	"minimum-stability" : "stable",
	"require" : {
		"php" : "^8.1",
		"php-64bit": "^8.1",
		"ext-json": "*",
		"ext-mbstring":"*",
		"ext-bcmath":"*",
		"yiisoft/yii2" : "~2.0",
		"yiisoft/yii2-redis": "^2.0",
		"taobig/php-helper": "^1.3 || ^2.0"
	},
	"require-dev": {
		"yiisoft/yii2-gii": "^2.2"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"autoload" : {
		"psr-4" : {
			"taobig\\yii\\" : "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"taobig\\tests\\yii\\": "tests/"
		}
	},
	"homepage": "https://github.com/taobig/yii-helper",
	"support": {
		"issues": "https://github.com/taobig/yii-helper/issues"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
